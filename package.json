    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "events": "^3.3.0",
    "fake-indexeddb": "^6.0.0",
    "idb": "^8.0.0",
    "lodash": "^4.17.21",
    "pinia": "^2.2.2",
    "quasar": "^2.17.0",
    "regenerator-runtime": "^0.14.1",
    "sanitize-html": "^2.13.0",
    "throttled-queue": "^2.1.4",
