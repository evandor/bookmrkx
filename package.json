{
  "name": "bookmrkx",
  "version": "0.0.3",
  "description": "Use the bookmrkx browser extension to open your bookmarks in the sidepanel",
  "productName": "Bookmrkx",
  "author": "evandor <evandor@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m bex",
    "build bex (publish)": "quasar build -m bex",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:watch": "vitest --watch",
    "test:unit:watchAll": "vitest --watchAll",
    "info": "yarn list > docs/yarn.list.txt && quasar info > docs/quasar.info.txt"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^5.0.0",
