{
  "name": "bookmrkx",
  "version": "0.0.1",
  "description": "Use the bookmrkx browser extension to open your bookmarks in the sidepanel",
  "productName": "Bookmrkx",
  "author": "evandor <evandor@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m bex",
    "build bex (publish)": "quasar build -m bex",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:watch": "vitest --watch",
    "test:unit:watchAll": "vitest --watchAll",
    "web-ext run": "cd dist/bex && web-ext run --verbose",
    "web-ext build": "quasar build -m bex &&  zip dist/bookmrkx.src.zip -r src/ src-bex/ package.json && cd dist/bex && web-ext build"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/extras": "^1.16.9",
    "@types/lodash": "^4.14.182",
    "@types/sanitize-html": "^2.11.0",
    "buffer": "^6.0.3",
    "date-fns": "^3.1.0",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "fake-indexeddb": "^5.0.2",
    "idb": "^8.0.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.11",
    "quasar": "2.15.2",
    "regenerator-runtime": "^0.14.1",
    "sanitize-html": "^2.10.0",
    "translate": "^3.0.0",
    "ts-md5": "^1.3.1",
    "uuid": "^9.0.0",
    "vite-plugin-package-version": "^1.1.0",
    "vue": "^3.4.18",
    "vue-draggable-next": "^2.1.1",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.0.0",
    "vue-timeago3": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@quasar/app-vite": "^1.8.0",
    "@quasar/babel-preset-app": "^2.0.2",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/node": "^20.12.5",
    "@types/uuid": "^9.0.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "@vue/test-utils": "^2.4.1",
    "@wdio/cli": "^8.13.5",
    "autoprefixer": "^10.4.2",
    "typescript": "^5.1.6",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
